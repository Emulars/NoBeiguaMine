{
    "name": "smui-example-sveltekit",
    "version": "0.0.1",
    "scripts": {
        "dev": "svelte-kit dev",
        "build": "svelte-kit build",
        "preview": "svelte-kit preview",
        "prepare": "svelte-kit sync",
        "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
        "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "theme": "npm run smui-light && npm run smui-dark"
    },
    "devDependencies": {
        "@beyonk/gdpr-cookie-consent-banner": "^9.0.3",
        "@material/theme": "^13.0.0",
        "@material/typography": "^13.0.0",
        "@mdi/js": "^6.6.96",
        "@smui/button": "^6.0.0-beta.16",
        "@smui/card": "^6.0.0-beta.16",
        "@smui/form-field": "^6.0.0-beta.16",
        "@smui/icon-button": "^6.0.0-beta.16",
        "@smui/switch": "^6.0.0-beta.16",
        "@smui/top-app-bar": "^6.0.0-beta.16",
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/kit": "next",
        "@types/jquery": "^3.5.14",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.7.0",
        "smui-theme": "^5.0.0",
        "svelte": "^3.47.0",
        "svelte-check": "^2.6.0",
        "svelte-mdc": "^0.2.5",
        "svelte-mui": "^0.5.0",
        "svelte-parallax": "^0.5.2",
        "svelte-preprocess": "^4.10.5",
        "tslib": "^2.3.1",
        "typescript": "^4.6.3"
    },
    "type": "module",
    "dependencies": {
        "@material/switch": "^14.0.0",
        "@smui/common": "^6.0.0-beta.16",
        "@svelte-web-fonts/google": "^1.0.4",
        "install": "^0.13.0",
        "jquery": "^3.6.0",
        "npm": "^8.11.0",
        "svelte-vertical-timeline": "^0.0.2",
        "svrollbar": "^0.12.0"
    }
}
